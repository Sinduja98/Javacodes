import java.util.*;
public class Main
{
	public static void main(String[] args) {
		//System.out.println("Hello World");
		PriorityQueue<Integer> minheap=new PriorityQueue<>();
		
		int[] arr={4,5,2,1,9,78};
		int k=3;
		for(int i=0;i<arr.length;i++){
		    minheap.add(arr[i]);
		}
		
		while(k>0){
		   System.out.println(minheap.poll());
		    k--;
		}
	}
}
